{
    "extends": [
        "react-app",
        "prettier"
    ],
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": [
            "warn"
        ],
        "no-console": "warn"
    },
    "settings": {
        "import/resolver": {
            "alias": [
                [
                    "@components",
                    "./src/dashboard/components"
                ],
                [
                    "@assets",
                    "./src/dashboard/assets"
                ],
                [
                    "@pages",
                    "./src/dashboard/pages"
                ]
            ]
        }
    }
}
